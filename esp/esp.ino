// STFT //

ArduinoFFT FFT = ArduinoFFT<float>();
float model_input[FFT_SIZE];
#include <Arduino.h>
#include <math.h>
#include <arduinoFFT.h>

static void hann_window(float* win, size_t n) {
	for (size_t i = 0; i < n; i++) {
		win[i] = 0.5f * (1.0f - cosf(2.0f * PI * i / (n - 1)));
	}
}

float window[FFT_SIZE];
float vReal[FFT_SIZE];
float vImag[FFT_SIZE];
void stft(int16_t* samples, size_t n_samples) {
	const size_t freq_bins = FFT_SIZE; // yeah it's redundant, so what

	size_t num_frames;
	if (n_samples < FFT_SIZE) {
		num_frames = 1;
	} else {
		num_frames = 1 + (size_t)ceilf((float)(n_samples - FFT_SIZE) / STFT_STEP);
	}

	hann_window(window, FFT_SIZE);

	for (size_t f = 0; f < num_frames; f++) {
		size_t start = f * STFT_STEP;

		// load frame with zero-padding + window
		for (size_t i = 0; i < FFT_SIZE; i++) {
			size_t idx = start + i;
			float sample = (idx < n_samples) ? (float)samples[idx] : 0.0f;
			vReal[i] = sample * window[i];
			vImag[i] = 0.0f;
		}

		// FFT
		// fft.Windowing(FFT_WIN_TYP_NONE, FFT_FORWARD); // window already applied
		// fft.Compute(FFT_FORWARD);
		// fft.ComplexToMagnitude();
		// FFT.windowing(vReal, FFT_SIZE, FFT_WIN_TYP_HANN, FFT_FORWARD);
		FFT.compute(vReal, vImag, FFT_SIZE, FFT_FORWARD);
		FFT.complexToMagnitude(vReal, vImag, FFT_SIZE);

		// magnitude -> dB
		for (size_t k = 0; k < freq_bins; k++) {
			float mag = vReal[k];
			float mag_db = 20.0f * log10f(mag + 1e-10f);
			model_input[k] += mag_db / num_frames;
		}
	}
}

