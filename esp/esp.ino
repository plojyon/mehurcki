float int2float(uint16_t h) {
	uint32_t sign = (uint32_t)(h & 0x8000) << 16;
	uint32_t exp  = (h >> 10) & 0x1F;
	uint32_t mant = h & 0x03FF;

	uint32_t bits;

	if (exp == 0) {
		if (mant == 0) {
			bits = sign; // +-0
		} else {
			// subnormal
			exp = 1;
			while ((mant & 0x0400) == 0) {
				mant <<= 1;
				exp--;
			}
			mant &= 0x03FF;
			uint32_t f_exp = (exp + (127 - 15)) << 23;
			uint32_t f_mant = mant << 13;
			bits = sign | f_exp | f_mant;
		}
	}
	else if (exp == 31) {
		// inf or NaN
		bits = sign | 0x7F800000 | (mant << 13);
	}
	else {
		uint32_t f_exp  = (exp + (127 - 15)) << 23;
		uint32_t f_mant = mant << 13;
		bits = sign | f_exp | f_mant;
	}

	float out;
	memcpy(&out, &bits, sizeof(out));
	return out;
}


float rbf_kernel(const float *x, const uint16_t *y) {
	float sum = 0.0f;
	for (int i = 0; i < N_FEATURES; i++) {
		float d = x[i] - int2float(y[i]);
		sum += d * d;
	}
	// Serial.print("Sum = ");
	// Serial.print(sum);
	float e = exp(-GAMMA * sum);
	// Serial.print("; Gamma*sum = ");
	// Serial.print(-GAMMA * sum);
	// Serial.print("; exp(-Gamma*sum) = ");
	// Serial.println(e, 10);
	return e;
}

float svm_predict(const float *x) {
	float result = 0.0f;

	for (int i = 0; i < N_SUPPORT; i++) {
		float k = rbf_kernel(x, support_vectors[i]);
		float coeff = int2float(dual_coef[i]);
		if (coeff > 1.0f || coeff < -1.0f) {
			Serial.print("Dubious coefficient ");
			Serial.print(i);
			Serial.print(" ");
			Serial.print(dual_coef[i]);
			Serial.print(" ");
			Serial.println(coeff);
		}
		result += int2float(dual_coef[i]) * k;
	}

	result += intercept;
	return result;
}

