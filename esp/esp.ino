void detector_setup() {
	Serial.print("Setting up SVM. Head of support_vectors[0]: ");
	Serial.print(support_vectors[0][0]);
	Serial.print(",");
	Serial.print(support_vectors[0][1]);
	Serial.print(",");
	Serial.print(support_vectors[0][2]);
	Serial.print(",");
	Serial.println(support_vectors[0][3]);
	Serial.print("Converted to floats: ");
	Serial.print(int2float(support_vectors[0][0]));
	Serial.print(",");
	Serial.print(int2float(support_vectors[0][1]));
	Serial.print(",");
	Serial.print(int2float(support_vectors[0][2]));
	Serial.print(",");
	Serial.println(int2float(support_vectors[0][3]));
	Serial.print("Intercept: ");
	Serial.println(intercept);
	Serial.print("Gamma: ");
	Serial.println(GAMMA, 10);
	Serial.print("Gamma + 1: ");
	Serial.println(GAMMA + 1.0f, 10);
	float bubble;
	
	Serial.println("=============== TRUE POSITIVE EXAMPLE ===============");
	Serial.println(sizeof(tp_sample) / sizeof(tp_sample[0]));
	stft(tp_sample, 4410);
	Serial.print("Head of model_input: ");
	Serial.print(model_input[0]);
	Serial.print(",");
	Serial.print(model_input[1]);
	Serial.print(",");
	Serial.print(model_input[2]);
	Serial.print(",");
	Serial.println(model_input[3]);
	bubble = svm_predict(model_input);
	Serial.print("SVM bubble score: ");
	Serial.println(bubble);

	Serial.println("=============== TRUE NEGATIVE EXAMPLE ===============");
	Serial.println(sizeof(tn_sample) / sizeof(tn_sample[0]));
	stft(tn_sample, 4410);
	Serial.print("Head of model_input: ");
	Serial.print(model_input[0]);
	Serial.print(",");
	Serial.print(model_input[1]);
	Serial.print(",");
	Serial.print(model_input[2]);
	Serial.print(",");
	Serial.println(model_input[3]);
	bubble = svm_predict(model_input);
	Serial.print("SVM bubble score: ");
	Serial.println(bubble);
}


/*****************************************************************************/
// Setup and Loop //

void setup() {
	// station mode
	WiFi.mode(WIFI_STA);
	WiFi.disconnect();
	delay(100);
	Serial.begin(115200);

	// Set up I2S
	i2s_install();
	i2s_setpin();
	i2s_start(I2S_PORT);

	mqtt_setup();
	detector_setup();

	Serial.println("Setup done!");

	pinMode(STATUS_LED, OUTPUT);
}

void loop() {
	if (wifi_connected) {
		digitalWrite(STATUS_LED, LOW);
		if (mqtt_connected) {
			mqttClient.loop();
			detector_loop();
		}
		else {
			mqtt_connect();
		}
	} else {
		digitalWrite(STATUS_LED, HIGH);
		connect_to_wifi();
	}

	wifi_loop();
}

